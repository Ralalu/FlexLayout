{
    "compilerOptions": {
      "emitDeclarationOnly": true,
      "declaration": true,
      "declarationDir": "types",
      "module": "ESNext",
      "target": "ESNext",
      "moduleResolution": "Bundler",
      "jsx": "react-jsx",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src"]
  }
  
